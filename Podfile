# Uncomment the next line to define a global platform for your project
platform :ios, '10.0'

# Comment the next line if you don't want to use dynamic frameworks
use_frameworks!

# Pods for Paradise
def my_pods
    pod 'Alamofire'
    pod 'SwiftyJSON', '~> 4.0'
    pod 'PPSPing', '~> 0.3.0'
    pod 'DeviceKit'
    pod 'CryptoSwift'
    pod 'SAMKeychain'
    pod 'JJFloatingActionButton'
    pod 'Firebase/Core'
    pod 'Fabric'
    pod 'Crashlytics'
    pod 'Firebase/Performance'
end
 
target 'newDubai' do
    my_pods
end
 
target 'newDubai-Dev' do
    my_pods
end

target 'newDubai-Ndbpp-Dev' do
    my_pods
end

target 'newDubai-Ndbppp-Dev' do
    my_pods
end
